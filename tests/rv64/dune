(cram
 (applies_to *)
 (deps
  (package rukaml)
  ;(package rukaml_rv64)
  ; %{project_root}/compiler.exe
  %{project_root}/back/rv64/RV64_compiler.exe
  %{project_root}/back/rv64/rukaml_stdlib.o
  ;
  ))

;;;;; empty

(rule
 (targets empty.s)
 (deps
  %{project_root}/back/rv64/RV64_compiler.exe
  (:src empty.ml))
 (mode
  (promote (until-clean)))
 (action
  (progn
   (run
    %{project_root}/back/rv64/RV64_compiler.exe
    -o
    %{targets}
    --no-start
    %{src}))))

(rule
 (targets empty.exe)
 (deps
  %{project_root}/back/rv64/RV64_compiler.exe
  (:src empty.s)
  %{project_root}/back/rv64/rukaml_stdlib.o)
 (mode
  (promote (until-clean)))
 (action
  (progn
   (run riscv64-linux-gnu-gcc-13 -g -c %{src} -o empty.o)
   (run
    riscv64-linux-gnu-gcc-13
    -g
    %{project_root}/back/rv64/rukaml_stdlib.o
    empty.o
    -o
    %{targets}))))

(cram
 (applies_to empty)
 (deps
  %{project_root}/back/rv64/rukaml_stdlib.o
  ./empty.exe
  empty.ml
  empty.s))

